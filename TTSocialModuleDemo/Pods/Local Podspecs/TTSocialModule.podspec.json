{
  "name": "TTSocialModule",
  "version": "0.5",
  "summary": "简单封装第三方平台的分享功能",
  "description": "封装「微信」「微博」等第三方平台SDK，目前支持分享功能。",
  "homepage": "https://github.com/keleyundou/TTSocialModule",
  "license": "MIT",
  "authors": "ColaBean",
  "platforms": {
    "ios": "8.0"
  },
  "source": {
    "git": "https://github.com/keleyundou/TTSocialModule.git",
    "tag": "0.5"
  },
  "requires_arc": true,
  "subspecs": [
    {
      "name": "Common",
      "source_files": [
        "TTSocialModule/TT{Behavior,SocialDefine}.h",
        "TTSocialModule/TT{SocialMessageObject,SocialResponse,SocialUtil}.{h,m}"
      ],
      "public_header_files": "TTSocialModule/TT{SocialMessageObject,SocialResponse,SocialUtil}.h"
    },
    {
      "name": "Shared",
      "source_files": "TTSocialModule/Shared",
      "public_header_files": "TTSocialModule/Shared/TT{WeChat,Weibo}Shared.{h}",
      "dependencies": {
        "TTSocialModule/Common": [

        ],
        "WechatOpenSDK": [
          "~> 1.8.4"
        ],
        "Weibo_SDK": [
          "~> 3.2.3"
        ]
      }
    },
    {
      "name": "Handler",
      "source_files": "TTSocialModule/Handler",
      "dependencies": {
        "TTSocialModule/Common": [

        ],
        "TTSocialModule/Shared": [

        ]
      }
    },
    {
      "name": "Manager",
      "source_files": "TTSocialModule/TTSocialManager.{h,m}",
      "dependencies": {
        "TTSocialModule/Common": [

        ],
        "TTSocialModule/Handler": [

        ]
      }
    }
  ]
}
